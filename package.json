{
  "name": "fp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "js:build": "tsc",
    "js:delete": "rm -rf ./build",
    "js:build:watch": "tsc --watch",
    "declaration:build": "tsc --declaration",
    "declaration:delete": "find ./build -type f -name \"*.d.ts\" -delete",
    "declaration:build:savely": "npm run declaration:delete; npm run declaration:build;",
    "docs:build": "typedoc source/index.ts",
    "docs:delete": "rm -rf ./docs",
    "all:build": "npm run js:delete; npm run docs:delete; npm run js:build; npm run declaration:build; npm run docs:build",
    "test": "mocha -r ts-node/register source/**/*test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.6",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.5.3"
  }
}
